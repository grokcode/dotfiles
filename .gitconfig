[user]
        email = jess@grokcode.com
[core]
        editor = emacs
        excludesfile = ~/.gitignore
[content]
        editor = emacs
[alias]
        co = checkout
        co-pr = !sh -c 'git fetch origin pull/$1/head:pr/$1 && git checkout pr/$1' -
        unstage = reset HEAD --
[push]
        default = matching
[credential]
	helper = osxkeychain
